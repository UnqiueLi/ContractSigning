{
  "easycom": {
    "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    {
      "path": "pages/tabbar/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tabbar/user/user",
      "style": {
        "navigationBarTitleText": "我的",
        "navigationStyle": "custom"
      }
    },
	{
	  "path": "pages/tabbar/create/create",
	  "style": {
	    "navigationBarTitleText": "创建任务",
	    "navigationStyle": "custom"
	  }
	}
	
  ],
  "subPackages": [
    {
      "root": "subpkg_index",
      "pages": []
    },
	{
	  "root": "subpkg_create",
	  "pages": [
		{
		  "path": "pages/person/person",
		  "style": {
		    "navigationBarTitleText": "添加个人",
		    "enablePullDownRefresh": false,
		    "navigationBarBackgroundColor": "#D4E2F8",
		    "navigationBarTextStyle": "black"
		  }
		},
		{
		  "path": "pages/enterprise/enterprise",
		  "style": {
		    "navigationBarTitleText": "添加企业",
		    "enablePullDownRefresh": false,
		    "navigationBarBackgroundColor": "#D4E2F8",
		    "navigationBarTextStyle": "black"
		  }
		}
	  ]
	},
    {
      "root": "subpkg_login",
      "pages": [
        {
          "path": "pages/login/login",
          "style": {
            "navigationBarTitleText": "",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#D4E2F8",
            "navigationBarTextStyle": "black"
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#5570F1",
    "borderStyle": "white",
    "backgroundColor": "#fff",
    "list": [
      {
        "pagePath": "pages/tabbar/index/index",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home-active.png",
        "text": "首页"
      },
	  {
	    "pagePath": "pages/tabbar/create/create",
	    "iconPath": "/static/tabbar/create.png",
	    "selectedIconPath": "static/tabbar/create-active.png",
	    "text": "创建"
	  },
      {
        "pagePath": "pages/tabbar/user/user",
        "iconPath": "static/tabbar/user.png",
        "selectedIconPath": "static/tabbar/user-active.png",
        "text": "我的"
      }
    ]
  }
}